      "/{coin}/api/stats": {
        "get": {
          "tags": [
            "Pools"
          ],
          "summary": "Get global pool stats",
          "parameters": [
            {
              "name": "coin",
              "in": "path",
              "required": true,
              "description": "The Coin Ticker of the desired pool. For example, BTC, ZEC, ZEN, SC, DCR, etc.",
              "schema": {
                "type": "string"
              }
            }
          ],
          "operationId": "getPoolStats",
          "responses": {
            "200": {
              "description": "Everything worked as expected.",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "difficulty": {
                        "type": "integer"
                      },
                      "pp_hash": {
                        "type": "integer",
                        "description": "TBD"
                      },
                      "pp_share": {
                        "type": "integer",
                        "description": "TBD"
                      },
                      "block_subsidy": {
                        "type": "integer",
                        "description": "Block reward in the coin minimum unit"
                      },
                      "tp": {
                        "type": "integer",
                        "description": "TBD"
                      },
                      "pool_difficulty": {
                        "type": "integer",
                        "description": "TBD"
                      },
                      "now": {
                        "type": "integer",
                        "description": "TBD"
                      },
                      "prev_hash": {
                        "type": "string",
                        "description": "TBD"
                      },
                      "total_miners": {
                        "type": "integer",
                        "description": "Total amount of miners that submitted at least 1 valid share in the last TBD hours."
                      },
                      "total_online": {
                        "type": "integer",
                        "description": "Total amount of miners currenlty submitting valid shares to the pool."
                      },
                      "valid_shares": {
                        "type": "integer",
                        "description": "Total amount of valid shares submitted over the last TBD hours."
                      },
                      "invalid_shares": {
                        "type": "integer",
                        "description": "Total amount of invalid shares submitted over the last TBD hours."
                      },
                      "accepted_shares": {
                        "type": "integer",
                        "description": "Total amount of accepted shares by the pool over the last TBD hours."
                      },
                      "rejected_shares": {
                        "type": "integer",
                        "description": "Total amount of rejected shares by the pool over the last TBD hours."
                      },
                      "timed_out_miners": {
                        "type": "integer",
                        "description": "TBD"
                      },
                      "pending_payouts": {
                        "type": "string",
                        "description": "Outstanding balances to be paid by the pool in the coin base unit."
                      },
                      "hashrate": {
                        "type": "number",
                        "description": "Current hashrate being pointed towards the pool."
                      },
                      "hashrate_24h": {
                        "type": "number",
                        "description": "Average hashrate pointed towards the pool in the last 24 hours."
                      },
                      "height": {
                        "type": "integer",
                        "description": "Current block height the pool is attempting to find a valid block solution."
                      },
                      "total_kickback": {
                        "type": "integer",
                        "description": "Donations made from Luxor's fee to support different projects."
                      },
                      "users": {
                        "type": "object",
                        "description": "Top 100 Anonymous miners hashrate stats and their worker's hashing data.",
                        "properties": {
                          "address": {
                            "type": "string"
                          },
                          "hashrate": {
                            "type": "number"
                          },
                          "hashrate_24h": {
                            "type": "number"
                          },
                          "valid_shares_count": {
                            "type": "integer"
                          },
                          "invalid_shares_count": {
                            "type": "integer"
                          },
                          "accepts_count": {
                            "type": "integer"
                          },
                          "rejects_count": {
                            "type": "integer"
                          },
                          "max_share_diff": {
                            "type": "integer"
                          },
                          "balance": {
                            "type": "integer"
                          },
                          "miners": {
                            "type": "object",
                            "description": "Worker related statistics",
                            "properties": {
                              "worker_name": {
                                "type": "string"
                              },
                              "affinity": {
                                "type": "string",
                                "description": "Stratum name to which the worker is connected"

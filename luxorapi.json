    "paths": {
      "/switch/equihash/stats": {
        "get": {
          "tags": [
            "Switch"
          ],
          "summary": "Get PPS rates, switching events and uplift data.",
          "operationId": "getSwitchMarket",
          "responses": {
            "200": {
              "description": "Everything worked as expected.",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "profit_switch_events": {
                        "description": "Profit switching events",
                        "type": "object",
                        "properties": {
                          "time": {
                            "description": "Timestamp of the profit switching event",
                            "type": "string"
                          },
                          "from_currency_profile_name": {
                            "type": "string"
                          },
                          "to_currency_profile_name": {
                            "type": "string"
                          },
                          "pps_rate": {
                            "description": "PPS at the time of the switching event",
                            "type": "integer"
                          }
                        }
                      },
                      "pps_rates": {
                        "description": "PPS rates per coin",
                        "type": "object",
                        "properties": {
                          "coin": {
                            "description": "Coin ticker. For example, ARRR, ZEC, ZEN or KMD.",
                            "type": "object",
                            "properties": {
                              "time": {
                                "type": "string"
                              },
                              "usd_price": {
                                "type": "integer"
                              },
                              "btc_price": {
                                "type": "integer"
                              },
                              "network_difficulty": {
                                "type": "number"
                              },
                              "pps_rate": {
                                "description": "PPS rate in milisatoshis (Bitcoin / 1e14).",
                                "type": "integer"
                              },
                              "pps_value": {
                                "description": "PPS rate in Altcoin minimum unit.",
                                "type": "integer"
                              }
                            }
                          }
                        }
                      },
                      "uplift_data": {
                        "description": "Get a timeseries comparsion of the Luxor Switch PPS rate vs a basecoin (ZEC)",
                        "type": "object",
                        "properties": {
                          "time": {
                            "type": "string"
                          },
                          "basecoin": {
                            "type": "integer"
                          },
                          "uplift": {
                            "type": "integer"
                          }
                        }
                      }
                    }
                  }
                }
              }
            },
            "404": {
              "description": "The requested resource doesn't exist."
            },
            "405": {
              "description": "The method used isn't supported."
            }
          }
        }
      },

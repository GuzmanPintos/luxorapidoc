                              "miner_type": {
                                "type": "string"
                              },
                              "hashrate": {
                                "type": "number"
                              },
                              "hashrate_1h": {
                                "type": "number"
                              },
                              "max_share_diff": {
                                "type": "integer"
                              },
                              "last_beat": {
                                "type": "integer"
                              },
                              "stale_share_count": {
                                "type": "integer"
                              },
                              "stale_share_diff": {
                                "type": "integer"
                              },
                              "valid_shares_count": {
                                "type": "integer"
                              },
                              "invalid_shares_count": {
                                "type": "integer"
                              },
                              "valid_unpaid_shares_diff": {
